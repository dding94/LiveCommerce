<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
  namespace="com.flab.livecommerce.infrastructure.order.persistence.mybatis.MyBatisOrderItemOptionGroupRepository">

  <insert id="save" useGeneratedKeys="true" keyProperty="id">
    INSERT INTO order_item_option_group (order_line_item_id, ordering, name)

    VALUES (#{orderLineItemId}, #{ordering}, #{name})
  </insert>
</mapper>